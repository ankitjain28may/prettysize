{
    "name": "ankitjain28may/prettysize",
    "description": "Convert bytes to other sizes for prettier logging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ankit Jain",
            "email": "ankitjain28may77@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "php-coveralls/php-coveralls": "^2.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Ankitjain28may\\Prettysize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ankitjain28may\\Prettysize\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
